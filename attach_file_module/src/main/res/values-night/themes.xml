<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AttachFileModule" parent="Theme.MaterialComponents.DayNight">
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black_light</item>
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black_light</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

        <item name="white">@color/white_dark</item>
        <item name="whiteTransparent">@color/whiteTransparent_dark</item>
        <item name="black">@color/black_dark</item>
        <item name="green">@color/green_dark</item>
        <item name="blue">@color/blue_dark</item>
        <item name="gray">@color/gray_dark</item>
        <item name="orange">@color/orange_dark</item>
        <item name="purple">@color/purple_dark</item>
        <item name="red">@color/red_dark</item>
        <item name="transparent">@color/transparent_dark</item>

        <item name="bottomSheetTransparent">@color/bottomSheetTransparent_dark</item>

    </style>
</resources>